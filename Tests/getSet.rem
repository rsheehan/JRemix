a person :
	create
		'name' : "Robert"

		getter
			'name'

		MY name : ; overrides getter
			'name' ⊕ 'name'

		MY name 'value' :
			'name' : "*" ⊕ 'value' ⊕ "*"

'P' : a person

test {
	"Overwritten user defined getter."
		'P' name
	… expected "RobertRobert"

	"User defined setter."
		'P' name "Peter"
		'P' name
	… expected "*Peter**Peter*"
}

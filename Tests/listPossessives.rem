a b :
	create
		'x' : 1
		'y' : 99

		getters/setters
			'x', 'y'

'Z' : a b


'list' : {
	a b
	{3, a b}
}

test {
	"Get simple possessive 1"
		'Z' x
	… expected 1

	"Get simple possessive 2"
		'Z' y
	… expected 99

	"Set simple possessive"
		'Z' y 2
		'Z' y
	… expected 2

	"Get list item possessive"
		('list' {1}) y
	… expected 99

	"Set list item possessive"
		('list' {1}) x 3
		('list' {1}) x
	… expected 3

	"Set list 2D item possessive"
		('list' {2}{2}) y 4
		('list' {2}{2}) y
	… expected 4
	
	"In binary operation"
		('list' {1}) x + ('list' {2}{2}) y
	… expected 7
}

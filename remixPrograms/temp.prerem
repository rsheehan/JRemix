
- Playing with libraries -

outside : "global value"

small : library[

	(x) plus (y) :[
		x ⊕ y]

	a b :[
		2]]

using (small)[
	hidden : "library value"

	result : (outside) plus (hidden)]

people : library[

	name (name) age (age) :[
		create[
			name : name
			age : age

			(me) to string :[
				name ⊕ ", " ⊕ age ⊕ " years old"]]]]

using (people)[
	Robert : name "Robert" age 64]

Robert ↲
result ↲
